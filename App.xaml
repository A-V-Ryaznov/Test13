<Application x:Class="LearnApp.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:LearnApp"
             ShutdownMode="OnLastWindowClose"
             StartupUri="GUI/Frame/WindowAuthorization.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Library/Styles/Color.xaml"/>
                <ResourceDictionary Source="/Library/Styles/TextBlock.xaml"/>
                <ResourceDictionary Source="/Library/Styles/Image.xaml"/>
                <ResourceDictionary Source="/Library/Styles/PasswordBox.xaml"/>
                <ResourceDictionary Source="/Library/Styles/TextBox.xaml"/>
                <ResourceDictionary Source="/Library/Styles/Button.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
